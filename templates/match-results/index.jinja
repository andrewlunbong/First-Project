  {% extends "base.jinja"%}
  
  {% block content %}
    
    

  
  <h1> Results</h1>

  {% for match_result in match_results %}
    <p>
    <b> {{ match_result.match.home_team.name }}</b> <b> {{ match_result.home_team_score }}</b> : <b> {{ match_result.away_team_score }}</b> <b> {{ match_result.match.away_team.name }} </b>
  {% endfor %}
    
 

  {% endblock %} 
